{
  "root": "libs/data-integrity",
  "sourceRoot": "libs/data-integrity/src",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/data-integrity",
        "tsConfig": "libs/data-integrity/tsconfig.lib.json",
        "packageJson": "libs/data-integrity/package.json",
        "main": "libs/data-integrity/src/index.ts",
        "extractLicenses": false,
        "assets": [

        ]
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "data-integrity:build",
        "watch": false,
        "inspect": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/data-integrity/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/data-integrity"
      ],
      "options": {
        "jestConfig": "libs/data-integrity/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
